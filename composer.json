{
    "name": "viitortest/addressable",
    "description": "Provide the address book to store multiple address at one place",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhaval Joshi",
            "email": "dhaval.joshi@viitor.cloud"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "illuminate/auth": "^5.0|^6.0|^7.0",
        "illuminate/container": "^5.0|^6.0|^7.0",
        "illuminate/contracts": "^5.0|^6.0|^7.0",
        "illuminate/database": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "viitortest\\addressable\\AddressesServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Viitortest\\Addressable\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
